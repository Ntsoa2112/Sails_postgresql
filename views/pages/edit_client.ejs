<form method="POST" action="/update_client/<%= client.id %>">
    <label for="name">First name:</label><br>
    <input type="text" id="name" name="name" value="<%= client.name %>"><br>
    <label for="lastname">Last name:</label><br>
    <input type="text" id="lastname" name="lastname" value="<%= client.lastname %>"><br>   
    <label for="age">Age:</label><br>
    <input type="number" id="age" name="age" value="<%= client.age %>"><br>
    <input type="submit" value="Envoyer">
  </form>